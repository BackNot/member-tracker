{
  "name": "member-tracker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "electron/main.mjs",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "electron:dev": "concurrently \"npm run dev\" \"npm run electron:start\"",
    "electron:start": "wait-on http://localhost:5173 && electron --experimental-loader=node:module --experimental-modules .",
    "electron:build": "npm run build && electron-builder",
    "postinstall": "electron-rebuild -f -w sqlite3"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "primeicons": "^7.0.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.4",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.25",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.member-tracker.app",
    "productName": "Member Tracker",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ]
  }
}
